<p id="notice"><%= notice %></p>

<h1>Diets</h1>

<table>
  <thead>
    <tr>
      <th>Data inicial</th>
      <th>Data final</th>
      <th>Peso atual</th>
      <th>Peso ideal</th>
      <th>Altura</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diets.each do |diet| %>
      <tr>
        <td><%= diet.data_inicial %></td>
        <td><%= diet.data_final %></td>
        <td><%= diet.peso_atual %></td>
        <td><%= diet.peso_ideal %></td>
        <td><%= diet.altura %></td>
        <td><%= link_to 'Show', diet %></td>
        <td><%= link_to 'Edit', edit_diet_path(diet) %></td>
        <td><%= link_to 'Destroy', diet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Diet', new_diet_path %>
